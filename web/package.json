{
  "name": "fantasia-web",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b",
    "preview": "vite preview",
    "test": "vitest run",
    "test:quiet": "vitest run --silent",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:verbose": "vitest run --coverage --reporter=verbose",
    "test:debug": "VITE_LOG_LEVEL=debug vitest run",
    "test:e2e": "npm run compose:up && node e2e/compose.e2e.mjs; npm run compose:down || npm run compose:down",
    "test:websocket:e2e": "vitest run src/__tests__/e2e/websocket-e2e.test.ts --reporter=verbose"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^2.1.5",
    "jsdom": "^24.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.5",
    "ws": "^8.19.0"
  }
}
