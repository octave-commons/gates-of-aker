{
  "name": "gates-of-aker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run dev --prefix web",
    "build": "npm run build --prefix web",
    "preview": "npm run preview --prefix web",
    "test": "npm run test --prefix web",
    "test:quiet": "npm run test:quiet --prefix web",
    "test:watch": "npm run test:watch --prefix web",
    "test:coverage": "npm run test:coverage --prefix web",
    "test:coverage:verbose": "npm run test:coverage:verbose --prefix web",
    "test:debug": "npm run test:debug --prefix web",
    "typecheck": "tsc --noEmit",
    "typecheck:web": "npm exec --prefix web tsc --noEmit",
    "lint": "npm run lint --prefix web",
    "backend:dev": "cd backend && clojure -M:server",
    "backend:watch": "cd backend && clojure -X:watch-server",
    "backend:test": "cd backend && clojure -X:test",
    "backend:coverage": "cd backend && clojure -X:coverage",
    "compose:up": "npm run compose:up --prefix web",
    "compose:down": "npm run compose:down --prefix web",
    "test:e2e": "npm run test:e2e --prefix web"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "workspaces": [
    "web"
  ]
}